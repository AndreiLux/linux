			};
		};

		cam_res_mgr_suspend: cam_res_mgr_suspend {
			/* RESET */
			mux {
				pins = "gpio25";
				function = "gpio";
			};

			config {
				pins = "gpio25";
				bias-pull-down;
				output-low;
				drive-strength = <2>; /* 2 MA */
			};
		};

		flash_led_active: flash_led_active {
			mux {
				pins = "gpio21","gpio22";
				function = "gpio";
			};

			config {
				pins = "gpio21","gpio22";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
