				};
			};
		};

		hrm_int {
			hrm_int_active: hrm_int_active {
				hrm_int {
					pins = "gpio121";
					drive-strength = <2>;
					bias-disable;
				};
			};
			hrm_int_suspend: hrm_int_suspend {
				hrm_int {
					pins = "gpio121";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};
	};
};
