 * inside this file.
 */

&soc {
	samsung,vibrator {
		samsung,freq_0 = <2 104>;	// m n
		samsung,freq_low = <2 156>;
		samsung,freq_mid = <2 125>;
		samsung,freq_high = <3 140>;
		samsung,freq_alert = <2 119>;
		samsung,strength_od = <70>;
		samsung,strength_default = <50>;
	};
};
